package com.NewProject;

public class Stringbuffer {
	public static void main(String[] args)

	{
		StringBuffer sb=new StringBuffer("hello");
		System.out.println(sb.capacity());
		sb.append("chennai");
		System.out.println(sb.capacity());
		
		sb.append("andhtra");
		sb.insert(3, "chewnnai");
		
		System.out.println(sb.capacity());
		sb.delete(2, 6);
	}
}