package inheritence;

import java.util.Scanner;

public class Excersize {
public static void main(String[] args) {
	
}
}
class Steve{
	String fname;
	String lname;
	int age;
	String courseName;
	int stuId;
	public void getData()
	{
		Scanner s=new Scanner(System.in);
		fname=s.nextLine();
		lname=s.nextLine();
		age=s.nextInt();
		courseName=s.nextLine();
		stuId=s.nextInt();
		
	}
	
}
class Employee extends Steve{
	String fname;
	String lname;
	int age;
	int sal;
	String depName;
	String designation;
	int empId;
	
	
}