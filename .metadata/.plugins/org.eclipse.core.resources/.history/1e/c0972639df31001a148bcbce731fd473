package inheritence;
public class Main{
	public static void main(String[] args) {
		Poly p=new Poly();
		p.area(2,3);
	}

public class Poly {
	
	
	public double area(double a){
		return a*a;
	}
	public double area(int a,double b){
		return a*b;
	}
	public double area(int a,int b) {
		return (a*b)/2;
	}
	
	}
}
